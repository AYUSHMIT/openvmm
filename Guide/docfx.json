{
  "build": {
    "content": [
      {
        "files": [ "**/*.md", "**/toc.yml" ]
      }
    ],
    "resource": [
      {
        "files": [
          "**/*.png"
        ]
      }
    ],
    "dest": "_site",
    "globalMetadata": {
      "_appTitle": "OpenVMM Developer Guide",
      "_enableSearch": "true",
      "_enableNewTab": "true",
      "_gitContribute": {
        "branch": "main"
      }
    },
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default"
    ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  }
}